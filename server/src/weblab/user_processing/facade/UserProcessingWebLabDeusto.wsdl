				</xsd:sequence>
			</xsd:complexType>


			<xsd:complexType name="CategoryType">
				<xsd:sequence>
					<xsd:element minOccurs="0" maxOccurs="1" name="name" type="xsd:string" />
				</xsd:sequence>
			</xsd:complexType>

			<xsd:complexType name="ExperimentType">
